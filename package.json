{
    "name": "facebook-api",
    "version": "1.0.0",
    "main": "server.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "copy": "copyfiles -f ./package.json ./dist/ && copyfiles -f ./config/* ./dist/config/",
        "build": "tsc -d -p . && npm run copy",
        "start": "npm run build && node ./dist/server.js",
        "nodemon": "npm run build && nodemon ./dist/server.js",
        "debug": "npm run build && node --inspect-brk ./dist/server.js"
    },
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.18.3",
        "config-json": "^1.0.0",
        "cors": "^2.8.4",
        "express": "^4.16.2",
        "fb": "^2.0.0",
        "fs": "0.0.1-security",
        "http": "0.0.0",
        "http-status": "^1.0.1",
        "log": "^1.4.0",
        "mongoose": "^5.0.4",
        "morgan": "^1.9.0",
        "request": "^2.88.0",
        "rootpath": "^0.1.2"
    },
    "devDependencies": {
        "@types/body-parser": "^1.16.8",
        "@types/express": "^4.11.1",
        "@types/http-status": "^0.2.30",
        "@types/mongoose": "^5.0.1",
        "@types/morgan": "^1.7.35",
        "@types/node": "^9.4.5",
        "nodemon": "^1.18.4",
        "typescript": "^2.7.1",
        "ws": "3.3.2"
